{
    "Logging": {
        "LogLevel": {
            "BuildCleaner.Support.RecursiveFolderLocator": "Information",
            "BuildCleaner.Rules.Selectors": "Information",
            "BuildCleaner": "Debug",
            "Default": "Debug",
            "System": "Information",
            "Microsoft": "Information"
        }
    },
    "Exclude": {
        "AncestorPath": true,
        "SymbolicLinks": true,
        "HiddenFolders": true
    },
    "Folders": {
        "Include": [
            "**/[Bb]in",
            "**/[Oo]bj",
            "**/testresults"
        ],
        "Exclude": [
            "**/.*",
            "**/node_modules",
            "?:/Windows/**",
            "/Library/**",
            "/Volumes/**",
            "/System/**",
            "/Users/*/Library/**",
            "/Applications/**"
        ]
    }
}